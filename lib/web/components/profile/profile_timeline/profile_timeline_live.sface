<div class={{"bg-white dark:bg-gray-700": e(assigns, :current_user, nil) != nil}}>
  <Bonfire.UI.Social.ProfileHeroFullLive user={{@user}} current_user={{e(assigns, :current_user, nil)}}  />
    <div class={{"mt-4 bg-white dark:bg-gray-700 shadow rounded-lg": e(assigns, :current_user, nil) == nil}}>
      <Bonfire.UI.Social.ProfileNavigationLive selected_tab={{@selected_tab}} user={{@user}} />
      {{ live_component(@socket, Bonfire.UI.Social.FeedLive, assigns_merge(assigns, id: "feed")) }}
    </div>
</div>
